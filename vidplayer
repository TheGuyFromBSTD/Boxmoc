function useitem(item)
    game:GetService("ReplicatedStorage").Events.PlayerActivesCommand:FireServer({["Name"] = item})        
end
function playvid(vid)
	task.wait(1)
	writefile('wow.mp4', game:HttpGet(vid))
    local screengui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
    local videoframe = Instance.new("VideoFrame", screengui)
    videoframe.Visible = true
    videoframe.Size = UDim2.new(1,0,1,0)
    videoframe.Video = getsynasset('wow.mp4')
    videoframe:Play()
    videoframe.Ended:Connect(function()
	    videoframe:Destroy()
    end)
end
function equipaccesory(item)
    local ohString1 = "Equip"
    local ohTable2 = {
        ["Category"] = "Accessory",
        ["Type"] = item
    }
    game:GetService("ReplicatedStorage").Events.ItemPackageEvent:InvokeServer(ohString1, ohTable2)
end
function equiptool(item)
    local ohString1 = "Equip"
    local ohTable2 = {
        ["Mute"] = true,
        ["Type"] = item,
        ["Category"] = "Collector"
    }
    game:GetService("ReplicatedStorage").Events.ItemPackageEvent:InvokeServer(ohString1, ohTable2)
end
local websocket = loadstring(game:HttpGet("https://raw.githubusercontent.com/TheGuyFromBSTD/Boxmoc/main/websocket"))()
local ws = websocket.new("ws://5.tcp.eu.ngrok.io:16373")
ws:init(game:GetService('Players').LocalPlayer.Name)

ws:register("reset","",function(...)
	local args = ...
	if game.Players.LocalPlayer.Name == args then
		game.Players.LocalPlayer.Character:BreakJoints()
	end
end)

ws:register("use","",function(...)
    local args = ...
    local splitargs = args:split(" ")
    if game.Players.LocalPlayer.Name == splitargs[1] then
        useitem(splitargs[2])
    end
end)
ws:register("equiptool","",function(...)
    local args = ...
    local splitargs = args:split(" ")
    if game.Players.LocalPlayer.Name == splitargs[1] then
        equiptool(splitargs[2])
    end
end)
ws:register("equipaccesory","",function(...)
    local args = ...
    local splitargs = args:split(" ")
    if game.Players.LocalPlayer.Name == splitargs[1] then
        equipaccesory(splitargs[2])
    end
end)
ws:register("playvid","",function(...)
    local args = ...
    playvid(args)
end)
getgenv().ws = ws
